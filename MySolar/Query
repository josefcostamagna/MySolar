CREATE TABLE `solardb`.`Utenti` ( `IdUtente` INT NOT NULL AUTO_INCREMENT , `Email` VARCHAR(30) NOT NULL , `Password` VARCHAR(30) NOT NULL , `Nome` VARCHAR(30) NOT NULL , `Cognome` VARCHAR(30) NOT NULL , `Data_Registrazione` DATE NOT NULL , `Indirizzo` VARCHAR(50) NULL , `Citt√†` VARCHAR(30) NULL , `Provincia` VARCHAR(2) NULL , `CAP` VARCHAR(6) NULL , `EmailConfermata` BIT NULL , PRIMARY KEY (`IdUtente`)) ENGINE = InnoDB;
